def longestCommonPrefix(strs):

    if not strs:
        return ""

    prefix = strs[0]
    for word in strs[1:]:
        while word.startswith(prefix) == False:
            prefix = prefix[:-1]
            if prefix == "":
                return ""

    return prefix


print(longestCommonPrefix(["dog", "dodge", "dominar"]))
